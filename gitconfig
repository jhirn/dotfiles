
[alias]
  st = status
  ll = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --max-count=50
  co = checkout
  br = branch
  df = diff --color
  ci = commit
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[ui]
  color = auto
[core]
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	editor = emacs
