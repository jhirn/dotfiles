{
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSaveMode": "modifications"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSaveMode": "modifications"
  },
  "autoimport.showNotifications": true,
  "breadcrumbs.enabled": true,
  "css.validate": false,
  "debug.console.fontSize": 14,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "editor.fontFamily": "Monaco, Menlo for Powerline, SF Mono, 'Courier New', monospace",
  "editor.fontWeight": "900",
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  "editor.hover.delay": 250,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.smoothScrolling": true,
  "editor.stickyTabStops": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "emacs-mcx.strictEmacsMove": true,
  "emacs-mcx.useMetaPrefixMacCmd": true,
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.nodeEnv": "",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.strings": "plist"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": true,
    "**/.git/hooks": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "tmp": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "tmp": true,
    "rspec_status.txt": true,
    "**/*.log": true,
    "dump*": true,
    "storage/**": true,
    "app/assets/builds/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "gitlens.blame.ignoreWhitespace": true,
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.format.onType.enabled": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.strictFunctionTypes": false,
  "json.format.enable": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "pgsql.resultsFontSize": 11,
  "redhat.telemetry.enabled": false,
  "ruby.codeCompletion": false,
  "ruby.format": false, // let solargraph format
  "ruby.intellisense": false,
  "ruby.specCommand": "bin/rspec",
  "ruby.specSaveFile": true,
  "ruby.useBundler": true,
  "ruby.useLanguageServer": false,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "security.workspace.trust.untrustedFiles": "open",
  "solargraph.autoformat": true,
  "solargraph.checkGemVersion": false,
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.logLevel": "debug",
  "solargraph.useBundler": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.lint.cssConflict": "error",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.profiles.osx": {
    "fish": {
      "path": "/opt/homebrew/bin/fish",
      "args": [
        "-l"
      ]
    }
  },
  "testExplorer.addToEditorContextMenu": true,
  "testExplorer.codeLens": false,
  "testExplorer.showOnRun": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.format.semicolons": "remove",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.nativeTabs": true,
  "workbench.commandPalette.history": 100,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.showTabs": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.plist": "default"
  },
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
  "python.defaultInterpreterPath": "python3",
  "[gemfile]": {
    "editor.defaultFormatter": "castwide.solargraph"
  },
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "cSpell.userWords": [
    "rubocop",
    "solargraph",
    "daisyui",
    "esbuild",
    "metafile",
    "outdir",
    "Roboto",
    "rubocop",
    "solargraph",
    "svgr",
    "usedapp",
    "walletconnect"
  ],
  "indentRainbow.errorColor": "rgba(111,111,11,0.3)",
  "vscode-postgres.prettyPrintJSONfields": true,
  "explorer.autoReveal": "focusNoScroll",
  "editor.foldingImportsByDefault": true,
  "editor.renderWhitespace": "all",
  "editor.cursorBlinking": "phase",
  "vscode-yaml-sort.emptyLinesUntilLevel": 1,
  "editor.codeLens": true,
  "workbench.colorCustomizations": {
    "editorWidget.background": "#21252BAA",
    "editorSuggestWidget.background": "#21252BCC",
    "editorSuggestWidget.foreground": "#FFFFFFAA",
    "editor.lineHighlightBackground": "#88888844",
    "editor.lineHighlightBorder": "#00000000"
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "search.useIgnoreFiles": false,
  "extensions.experimental.affinity": {
    "pub.name": 1
  },
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "powershell.promptToUpdatePowerShell": false,
  "editor.insertSpaces": true,
  "[python]": {},
  "shellformat.useEditorConfig": true,
  "editor.accessibilitySupport": "off",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "vs-browser.localProxyServer.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "rubyLsp.rubyVersionManager": "rbenv",
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": false
  },
  "[plist]": {},
  "settingsSync.keybindingsPerPlatform": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.columnSelection": true,
}
